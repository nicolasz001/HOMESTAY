<?xml version='1.0' encoding='utf-8'?>
<widget id="th.ac.up.ict.se.saraburi.farm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Home Stay</name>
    <description>
       Home Stay
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-navigation href="https://ec2-13-250-40-240.ap-southeast-1.compute.amazonaws.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="xxhdpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="xxxhdpi" src="res/icon/android/icon-36-ldpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <preference name="StatusBarBackgroundColor" value="#7951ef" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
